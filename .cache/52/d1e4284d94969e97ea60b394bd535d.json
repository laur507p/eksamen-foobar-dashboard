), url(\"fonts/mister_pixel_regular-webfont.woff\") format(\"woff\");\n  font-weight: normal;\n  font-style: normal;\n}\n\n@font-face {\n  font-family: \"vtf_misterpixeltools\";\n  src: url(\"fonts/mister_pixel_tools-webfont.woff2\") format(\"woff2\"), url(\"fonts/mister_pixel_tools-webfont.woff\") format(\"woff\");\n  font-weight: normal;\n  font-style: normal;\n}\n\nh1 {\n  font-family: \"vtf_misterpixelregular\";\n  font-weight: normal;\n  font-style: normal;\n}\n\nh2 {\n  font-family: \"vtf_misterpixelregular\";\n  font-weight: normal;\n  font-style: normal;\n  background-color: vars.$blue;\n  padding: 15px;\n  font-size: 1.2rem;\n  color: vars.$white;\n}\n\n// .title-container {\n//   position: relative;\n//   background-color: blue;\n//   width: 100%;\n//   height: 20px;\n//   padding: 0;\n// }\n\n// .title-container h2 {\n//   position: absolute;\n// }\n","sass/base/_variables.scss":"//colors\n\n// blue theme\n$blue: #1e00ff;\n$white: #ffffff;\n\n.green-theme {\n  $blue: green;\n  $white: red;\n}\n","sass/layout/_general.scss":"@use './sass/base/variables' as vars;\n\nbody {\n  height: 100vh;\n  width: 100vw;\n  padding: 40px;\n  background-image: url(\"images/bg-img-placeholder.jpg\");\n  background-size: cover;\n}\n\n#grid {\n  display: grid;\n  grid-template-columns: repeat(11, 1fr);\n  grid-template-rows: repeat(8, 1fr);\n  grid-column-gap: 15px;\n  grid-row-gap: 15px;\n  background-color: vars.$white;\n  height: 100%;\n  width: 100%;\n  border: 5px solid vars.$blue;\n  border-top: 25px solid vars.$blue;\n  padding: 15px;\n}\n\n.comp {\n  background-color: vars.$white;\n  border: 4px solid vars.$blue;\n}\n.comp1 {\n  grid-area: 1 / 1 / 2 / 3;\n  border: none;\n}\n.comp2 {\n  grid-area: 2 / 1 / 4 / 3;\n  background-color: vars.$blue;\n}\n.comp3 {\n  grid-area: 1 / 3 / 4 / 12;\n  display: flex;\n  flex-direction: column;\n  height: 100%;\n}\n.comp4 {\n  grid-area: 4 / 1 / 9 / 3;\n}\n.comp5 {\n  grid-area: 4 / 3 / 9 / 6;\n  display: flex;\n  flex-direction: column;\n  height: 100%;\n}\n.comp6 {\n  grid-area: 4 / 6 / 9 / 9;\n}\n.comp7 {\n  grid-area: 4 / 9 / 9 / 12;\n  padding: 0;\n}\n\n// theme select\n#theme-select {\n  position: fixed;\n  top: 0;\n  left: 0;\n  background-color: vars.$blue;\n  border: none;\n  outline: none;\n  padding: 5px;\n  color: vars.$white;\n  font-family: \"vtf_misterpixelregular\";\n  font-weight: normal;\n  font-style: normal;\n}\n\n// animations\n\n.fadeinout {\n  animation-name: fadeinout_kf;\n  animation-duration: 10s;\n  animation-timing-function: linear;\n  animation-fill-mode: both;\n}\n\n@keyframes fadeinout_kf {\n  0% {\n    opacity: 0;\n  }\n  10% {\n    opacity: 1;\n  }\n  90% {\n    opacity: 1;\n  }\n  100% {\n    opacity: 0;\n  }\n}\n","sass/components/_stockstatus.scss":"@use './sass/base/variables' as vars;\n\n// storage\n.storage-meter {\n  height: 20px;\n  color: vars.$white;\n  position: relative;\n  background-color: vars.$blue;\n  transition: 1s;\n}\n\n.inner {\n  position: absolute;\n  top: 0;\n  left: 0;\n  background-color: vars.$blue;\n  width: 50px;\n  height: 20px;\n}\n\n.outer {\n  position: absolute;\n  top: 0;\n  left: 0;\n  height: 20px;\n  width: 100%;\n  border: 1px solid red;\n}\n\n.storage-item {\n  margin: 10px 0;\n}\n\n.storage-container {\n  overflow-y: scroll;\n  height: 95%;\n  padding: 15px;\n  flex: 1;\n}\n","sass/components/_orders.scss":"@use './sass/base/variables' as vars;\n\n// parent container\n.orders-container {\n  display: flex;\n  overflow-x: scroll;\n  flex: 1;\n  transition: 1s;\n}\n\n// child container\n.order-container {\n  background-color: vars.$blue;\n  color: vars.$white;\n\n  min-width: 400px;\n  margin: 15px 0 15px 15px;\n  transition: 1s;\n}\n"},"lineCount":null}},"error":null,"hash":"6a078797a970310c37c5b81500a26101","cacheData":{"env":{}}}